{
  "name": "Notify",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [{ "matches": ["https://www.amazon.com/*","https://*.souq.com/*"], "js": ["content.js"] }],
  "browser_action":{
      "default_popup":"popup.html",
      "default_title":"Notify"
  },
  "permissions": [
    "tabs",
    "http://*/"
]
}
